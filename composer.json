{
    "name": "ilgala/laravel-wizzy",
    "type": "package",
    "description": "A laravel install wizard",
    "keywords": ["laravel","package","install","wizard","jquery"],
    "license": "MIT",
    "authors": [
        {
            "name": "IlGala",
            "email": "filippo.galante@b-ground.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*",
        "illuminate/foundation": "5.1.*|5.2.*|5.3.*",
        "illuminate/http": "5.1.*|5.2.*|5.3.*",
        "illuminate/routing": "5.1.*|5.2.*|5.3.*"
    },    
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "autoload": {
        "psr-4": {
            "IlGala\\LaravelWizzy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IlGala\\LaravelWizzy\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v1.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
